        "from PIL import Image, ImageDraw\n",
        "\n",
        "# Background dimensions\n",
        "background_width, background_height = 1700, 2200\n",
        "\n",
        "# Create a blank image\n",
        "image = Image.new(\"RGB\", (background_width, background_height), \"white\")\n",
        "draw = ImageDraw.Draw(image)\n",
        "\n",
        "# Define YOLO format coordinates for Label 1 and Label 2\n",
        "label1_yolo = (0.5011764705882353, 0.5854545454545454, 0.8847058823529412, 0.05454545454545454)\n",
        "label2_yolo = (0.25, 0.5854545454545454, 0.3247058823529412, 0.04363636363636364)\n",
        "\n",
        "# Convert YOLO format to pixel coordinates\n",
        "x1_label1 = (label1_yolo[0] - label1_yolo[2] / 2) * background_width\n",
        "y1_label1 = (label1_yolo[1] - label1_yolo[3] / 2) * background_height\n",
        "x2_label1 = (label1_yolo[0] + label1_yolo[2] / 2) * background_width\n",
        "y2_label1 = (label1_yolo[1] + label1_yolo[3] / 2) * background_height\n",
        "\n",
        "x1_label2 = (label2_yolo[0] - label2_yolo[2] / 2) * background_width\n",
        "y1_label2 = (label2_yolo[1] - label2_yolo[3] / 2) * background_height\n",
        "x2_label2 = (label2_yolo[0] + label2_yolo[2] / 2) * background_width\n",
        "y2_label2 = (label2_yolo[1] + label2_yolo[3] / 2) * background_height\n",
        "\n",
        "# Draw rectangles for Label 1 and Label 2\n",
        "draw.rectangle([x1_label1, y1_label1, x2_label1, y2_label1], outline=\"red\")\n",
        "draw.rectangle([x1_label2, y1_label2, x2_label2, y2_label2], outline=\"blue\")\n",
        "\n",
        "# Save the image for inspection\n",
        "image.save(\"image_with_yolo_boxes.png\")\n",
        "image.show()"
      ],
