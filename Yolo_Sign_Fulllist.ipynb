        "from PIL import ImageEnhance\n",
        "def paste_on_background(background_path, cropped_image,label2_relative_coords, output_image_path,output_box_path, position):\n",
        "\n",
        "    background = Image.open(background_path)\n",
        "    background_width, background_height = background.size\n",
        "    print(background_width, background_height)\n",
        "    cropped_image = cropped_image.convert(\"RGBA\")\n",
        "    background = background.convert(\"RGBA\")\n",
        "\n",
        "\n",
        "    # Enhance brightness and contrast if needed\n",
        "    enhancer = ImageEnhance.Brightness(cropped_image)\n",
        "    cropped_image = enhancer.enhance(1.2)  # Adjust the factor as needed\n",
        "\n",
        "    enhancer = ImageEnhance.Contrast(cropped_image)\n",
        "    cropped_image = enhancer.enhance(1.2)\n",
        "\n",
        "\n",
        "    # Create a copy of the background for drawing the bounding box\n",
        "    background_with_box = background.copy()\n",
        "\n",
        "\n",
        "    # Paste the cropped image onto the background at the specified position\n",
        "    background.paste(cropped_image, position, cropped_image if cropped_image.mode == 'RGBA' else None)\n",
        "    background_with_box.paste(cropped_image, position, cropped_image if cropped_image.mode == 'RGBA' else None)\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "    # Draw bounding box for visualization (optional)\n",
        "    draw = ImageDraw.Draw(background_with_box)\n",
        "    x1, y1 = position\n",
        "    x2, y2 = x1 + cropped_image.width, y1 + cropped_image.height\n",
        "    draw.rectangle([x1, y1, x2, y2], outline=\"red\")\n",
        "\n",
        "\n",
        "    x1_label2_rel, y1_label2_rel, x2_label2_rel, y2_label2_rel = label2_relative_coords\n",
        "    x3, y3=  x1 + x1_label2_rel, y1 + y1_label2_rel\n",
        "    x4, y4 = x1 + x2_label2_rel, y1 + y2_label2_rel\n",
        "    draw.rectangle([x3, y3, x4, y4], outline=\"blue\")\n",
        "\n",
        "\n",
        "\n",
        "    print(\"co-ordinates with respect to background image:\",x1, y1, x2, y2)\n",
        "    print(\"co-ordinates with respect to background image:\",x3, y3, x4, y4)\n",
        "    # Save the result\n",
        "    background.save(output_image_path)\n",
        "    background_with_box.save(output_box_path.replace('.png', '_bbox.png'))\n",
        "    print(f\"Saved images to {output_image_path} and {output_box_path.replace('.png', '_bbox.png')}\")\n",
        "\n",
        "\n",
        "    # Calculate YOLO format coordinates\n",
        "    bbox_x_center = (x1 + x2) / 2 / background_width\n",
        "    bbox_y_center = (y1 + y2) / 2 / background_height\n",
        "    bbox_width = cropped_image.width / background_width\n",
        "    bbox_height = cropped_image.height / background_height\n",
        "\n",
        "\n",
        "    bbox2_x_center = (x3 + x4) / 2 / background_width\n",
        "    bbox2_y_center = (y3 + y4) / 2 / background_height\n",
        "    bbox2_width = (x4 - x3) / background_width\n",
        "    bbox2_height =(y4 - y3) / background_height\n",
        "\n",
        "    yolo_format = (bbox_x_center, bbox_y_center, bbox_width, bbox_height)\n",
        "    yolo_format_2 = (bbox2_x_center, bbox2_y_center, bbox2_width, bbox2_height)\n",
        "    print(yolo_format,yolo_format_2)\n",
        "    return (yolo_format,yolo_format_2)"
